{
  "name": "moomin-view",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "browser": "lib/index.js",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib"
  ],
  "scripts": {
    "coverage": "npm run test -- --coverage",
    "start": "start-storybook -p 6060",
    "commit": "git-cz",
    "test": "jest --watch",
    "clean": "rm -rf node_modules && npm i",
    "prepare": "npm run build",
    "build": "rollup -c",
    "build:storybook": "build-storybook",
    "deploy": "gh-pages -d storybook-static",
    "predeploy": "npm run build-storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.story.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jext/fileMock.js",
      "\\.(css|sass|scss)$": "<rootDir>/config/jest/cssMock.js"
    }
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-native": "^0.60.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
